#include<iostream>
using namespace std;
int main (){
    //Make Array of Size 5
    char arry[5];
    //Loop for taking  and storing  five characters
    cout<<"Enter 5 Characters:\n";
    for(int i=0;i<=4;i++)
    {
      cin>>arry[i];
    }
    //Loop for printing stored five characters
    cout<<"Entered 5 characters are:\n";
    for(int i=0;i<=4;i++)
    {
        cout<<arry[i]<<endl;
    }
    return 0;
}
